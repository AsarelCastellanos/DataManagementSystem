<% for(var i = 0; i < stories.length; i++) { %>
<tr>
  <td><%= stories[i].idNum %></td>
  <td><%= stories[i].name %></td>
  <td><%= stories[i].condition %></td>
  <td><%= stories[i].dateAdded %></td>
  <td><%= stories[i].content %></td>
  <td><%= stories[i].externalLink %></td>
  <td class="action_button">
    <a href="/update-story?id=<%= stories[i]._id%>" class="btn action border-shadow update">
      <span class="text-gradient">
        <i class="fas fa-pencil-alt"></i>
      </span>
    </a>
    <a class="btn action border-shadow delete" data-id="<%= stories[i]._id%>">
      <span class="text-gradient">
        <i class="fas fa-times"></i>
      </span>
    </a>
  </td>
</tr>
<% } %>